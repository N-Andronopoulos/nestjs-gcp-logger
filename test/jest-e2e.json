{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@tazgr/nestjs-gcp-logger/(.*)": "<rootDir>/../src/$1",
    "@tazgr/nestjs-gcp-logger": "<rootDir>/../src"
  }
}
